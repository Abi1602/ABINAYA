#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include<ctype.h>

int main() {
    char str[100];
    int j=0;
    fgets(str,100,stdin);
    for(int i=0;str[i]!='\0';i++){
        char ch=tolower(str[i]);
    
        if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'){
            j++;
        }
    }printf("%d\n",j);
    return 0;
}
