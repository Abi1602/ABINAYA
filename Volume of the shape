#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#define PI 3.1416

int main() {
    int l,b,h,r;
    scanf("%d%d%d%d",&l,&b,&h,&r);
    float volumeCuboid=l*b*h;
    float volumeCylinder=PI*r*r*h;
    float volumeCone=(PI*r*r*h)/3;
    float volumeSphere=(4.0/3.0)*PI*r*r*r;
    printf("Volume of Cubiod=%.2f\n",volumeCuboid);
    printf("Volume of Cylinder=%.2f\n",floor(volumeCylinder*100)/100);
    printf("Volume of Cone=%.2f\n",floor(volumeCone*100)/100);
    printf("Volume of Shpere=%.2f\n",floor(volumeSphere*100)/100);

    return 0;
}
