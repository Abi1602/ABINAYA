#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    char str[100];
    fgets(str,sizeof(str),stdin);
    int len=0;
    for(int i=0;str[i]!='\0';i++){
        len++;
    }
    for(int i=len-1;i>=0;i--){
        printf("%c",str[i]);
    }
    return 0;
}
