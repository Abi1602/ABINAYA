#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    if(n>0&&n<200)
    {
        printf("The Given Number %d is Positive.\n",n);
    }
    else if(n<0){
        printf("The Given Number %d is Negative.\n",n);
    }
    else{
        printf("The Given Number %d is Zero.\n",n);
    }
    return 0;
}
